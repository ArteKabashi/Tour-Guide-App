<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tourguideapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/albanian1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Prizren"
            android:parentActivityName=".Prizren"></activity>
        <activity android:name=".Vlore"
            android:parentActivityName=".Vlore"/>
        <activity
            android:name=".Tirana"
            android:parentActivityName=".Tirana" />
        <activity
            android:name=".Info"
            android:parentActivityName=".Info" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>